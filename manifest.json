{
  "name": "Bootstrap Grid Overlay",
  "description": "Simple overlay, which shows the current Boostrap's grid",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "javascripts/background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_title": "Click to show an alert"
  }
}
